{
  "_comment": "http://browsenpm.org/package.json",
  "name": "JScandy",
  "version": "0.1.0",
  "scripts": {
    "build": "browserify -t reactify --debug js/app.js -o js/bundle.js",
    "start": "watchify -t reactify --debug  js/app.js -o js/bundle.js"
  },
  "main": "js/app.js",
  "browserify": {
    "transform": [ "reactify" ]
  },
  "repository": {
  	"type": "git",
  	"url": "https://github.com/jameshadfield/JScandy"
  },
  "bugs": {
  	"url": "https://github.com/jameshadfield/JScandy/issues"
  },
  "dependencies": {
    "browserify": "10.2.4",
    "reactify": "1.1.1",
    "watchify": "3.2.3",
    "react": "0.13.3",
    "events": "1.0.2",
    "object-assign": "3.0.0"
  },
  "preferGlobal": false,
  "private": true,
  "license": "MIT"
}
