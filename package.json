{
  "_comment": "http://browsenpm.org/package.json",
  "name": "JScandy",
  "version": "0.2.0",
  "scripts": {
    "build": "browserify js/app.js -t reactify --debug -p [minifyify --no-map] -o js/bundle.js",
    "notWorking": "watchify js/app.js -t reactify --debug --verbose -p [minifyify --map js/bundle.map.json --output js/bundle.map.json] -o js/bundle.js",
    "watch": "watchify js/app.js -t reactify --debug --verbose -o js/bundle.js"
  },
  "main": "js/app.js",
  "browserify": {
    "transform": [
      "reactify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jameshadfield/JScandy"
  },
  "bugs": {
    "url": "https://github.com/jameshadfield/JScandy/issues"
  },
  "dependencies": {
    "browserify": "latest",
    "events": "latest",
    "minifyify": "latest",
    "object-assign": "latest",
    "phylocanvas-plugin-context-menu": "phylocanvas/phylocanvas-plugin-context-menu",
    "react": "latest",
    "react-dom": "^0.14.0",
    "reactify": "latest",
    "watchify": "latest"
  },
  "preferGlobal": false,
  "private": true,
  "license": "MIT"
}
