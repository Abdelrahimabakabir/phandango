{
  "_comment": "http://browsenpm.org/package.json",
  "name": "JScandy",
  "version": "0.2.3",
  "scripts": {
    "build": "browserify js/app.js -t reactify --debug -p [minifyify --no-map] -o js/bundle.js",
    "notWorking": "watchify js/app.js -t reactify --debug --verbose -p [minifyify --map js/bundle.map.json --output js/bundle.map.json] -o js/bundle.js",
    "watch": "watchify js/app.js --debug --verbose -o js/bundle.js",
    "start": "http-server --cors -a 127.0.0.1 -p 8000"
  },
  "main": "js/app.js",
  "browserify": {
    "transform": [
      ["babelify", {"presets": ["es2015", "react"]}]
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jameshadfield/JScandy"
  },
  "bugs": {
    "url": "https://github.com/jameshadfield/JScandy/issues"
  },
  "dependencies": {
    "browserify": "latest",
    "eslint": "latest",
    "babel-eslint": "latest",
    "eslint-plugin-react": "3.11.2",
    "events": "1.1.0",
    "http-server": "0.8.5",
    "material-ui": "0.13.4",
    "minifyify": "7.1.0",
    "object-assign": "4.0.1",
    "phylocanvas-plugin-context-menu": "phylocanvas/phylocanvas-plugin-context-menu",
    "react": "0.14.3",
    "react-dom": "0.14.3",
    "react-tap-event-plugin": "0.2.1",
    "watchify": "3.6.1",
    "babel-preset-es2015": "latest",
    "babel-preset-react": "latest",
    "babelify": "latest"
  },
  "preferGlobal": false,
  "private": true,
  "license": "MIT"
}
